{
    "list": {
        "!collapsible": {
            "display": "[[collapsible]]",
            "value": ["[[collapsible show=\\\"+ open\\\" hide=\\\"- close\\\"]]\\n", "\\n[[/collapsible]]"]
        },
        "!colmod": {
            "display": "[[...colmod]]",
            "value": ["[[include :scp-int:component:coltop show=+open|hide=-close]]\\n", "\\n[[include :scp-int:component:colend]]"]
        },
        "!tabview": {
            "display": "[[tabview]]",
            "value": ["[[tabview]]\\n[[tab title]]\\n", "\\n[[/tab]]\\n[[/tabview]]"]
        },
        "!size": {
            "display": "[[size]]",
            "value": ["[[size 120%]]", "[[/size]]"]
        },
        "!footnote": {
            "display": "[[footnote]]",
            "value": ["[[footnote]]", "[[/footnote]]"]
        },
        "!ruby": {
            "display": "[[...ruby]]",
            "value": ["[[span class=\\\"ruby\\\"]]", "[[span class=\\\"rt\\\"]]ruby[[/span]][[/span]]"]
        },
        "!image": {
            "display": "[[...image]]",
            "value": ["[[include component:image-block\\n|name=<file name>\\n|caption=", "\\n]]"]
        },
        "!color": {
            "display": "##color##",
            "value": ["###b01|", "##"]
        },
        "!black": {
            "display": "blackbox(█)",
            "value": ["██", ""]
        }
    }
}
