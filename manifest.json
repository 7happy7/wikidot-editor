{
    "name": "Wikidot - Sandbox Editor",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "CHROME EXTENTION FOR RECOMMENDING INPUT CANDIDATE ON YOUR SANDBOX-PAGE",
    "author": "7happy7",
    "permissions": ["tabs", "storage", "unlimitedStorage", "*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
    "web_accessible_resources": ["assets/main.js"],
    "content_scripts": [
        {
            "matches": ["*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
            "js": ["assets/main.js"],
            "run_at": "document_start"
        }
    ]
}
