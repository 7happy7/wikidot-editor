{
    "name": "Wikidot - 沙盒編輯器",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "在沙盒中使用指令快速插入代碼的Chrome擴充功能。",
    "author": "7happy7",
    "permissions": ["storage", "*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
    "web_accessible_resources": ["assets/main.js", "assets/*.json"],
    "content_scripts": [
        {
            "matches": ["*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
            "js": ["assets/main.js"],
            "run_at": "document_start"
        }
    ]
}
